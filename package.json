{
  "name": "kekule-clj",
  "version": "0.7.0",
  "description": "A Kekule widget for Common Lisp Jupyter",
  "keywords": [
    "kekule",
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "widgets"
  ],
  "files": [
     "lib/**",
     "css/**",
     "LICENSE.md"
  ],
  "homepage": "https://github.com/yitzchak/kekule-clj",
  "bugs": {
    "url": "https://github.com/yitzchak/kekule-clj/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Tarn W. Burton",
    "email": "twburton@gmail.com"
  },
  "main": "lib/index.js",
  "types": "./lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/yitzchak/kekule-clj#no-cursors"
  },
  "scripts": {
    "build": "tsc",
    "lab-install": "yarn pack -f kekule-clj.tgz && jupyter-labextension install --no-build kekule-clj.tgz && sleep 5 && jupyter-lab build",
    "lint": "eslint . --ext .ts --fix",
    "lint-check": "eslint . --ext .ts",
    "prepack": "yarn run build"
  },
  "dependencies": {
    "@jupyter-widgets/base": "^6.0.1",
    "kekule": "https://github.com/yitzchak/Kekule.js.git"
  },
  "devDependencies": {
    "@phosphor/application": "^1.7.3",
    "@phosphor/widgets": "^1.9.3",
    "@types/node": "^20.2.1",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-standard": "^5.0.0",
    "lint-staged": "^13.0.3",
    "typescript": "^4.0.5"
  },
  "jupyterlab": {
    "extension": "lib/plugin"
  },
  "lint-staged": {
    "*.ts": [
      "eslint . --ext .ts --fix"
    ]
  },
  "prettier": {
    "singleQuote": true
  }
}
